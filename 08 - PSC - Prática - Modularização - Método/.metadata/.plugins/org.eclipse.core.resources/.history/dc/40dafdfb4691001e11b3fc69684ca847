package lista08_ex02;

public class Soma {

	float total = 0;
	String continuar;

	public float soma(float n) {
		this.continuar = c;
		while (this.continuar.equalsIgnoreCase("SIM")) {
			this.total += n;
			System.out.println("Deseja continuar?");
			this.continuar = c;
		}
		return this.total;
	}

}
