package exercicio;

import java.util.Scanner;

public class Exercicio {
	public static void main(String[] args) {
		Scanner t = new Scanner(System.in);
		System.out.println("numero ");

		int numero = t.nextInt();

		for (int i = 0; i <= 10; i++) {
			System.out.println(numero + " x " + i + " = " + (numero * i));
		}
t.close();
	}
}
