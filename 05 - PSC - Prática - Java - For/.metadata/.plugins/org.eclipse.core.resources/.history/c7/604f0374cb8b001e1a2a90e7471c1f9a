package exercicio;

import java.util.Scanner;

public class Exercicio {
	public static void main(String[] args) {
		int numero = 1;
		Scanner t = new Scanner(System.in);
		
		System.out.println("numero: ");
		numero = t.nextInt();
		

		for (int i = 0; i < numero; i++) {

			if(i%2 == 1) {System.out.println(i);}
		}
		
	}
}
